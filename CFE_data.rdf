	<dc:title>MNU 4-10a</dc:title>
	<sqa:refID>tcm:4-512886</sqa:refID>
</rdf:Description>

<rdf:Description rdf:about="http://localhost:3333/127">
	<rdf:type rdf:resource="http://data.sqa.org.uk/curriculumNode"/>
	<sqa:cnID>1438</sqa:cnID>
	<dc:description>I can use the link between time, speed and distance to carry out related calculations.</dc:description>
	<dc:title>MNU 4-10b</dc:title>
	<sqa:refID>tcm:4-512888</sqa:refID>
</rdf:Description>

<rdf:Description rdf:about="http://localhost:3333/128">
