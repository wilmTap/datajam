	<dc:title>MNU 2-10a</dc:title>
	<sqa:refID>tcm:4-512882</sqa:refID>
</rdf:Description>

<rdf:Description rdf:about="http://localhost:3333/123">
	<rdf:type rdf:resource="http://data.sqa.org.uk/curriculumNode"/>
	<sqa:cnID>1434</sqa:cnID>
