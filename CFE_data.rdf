	<dc:title>MNU 2-10b</dc:title>
	<sqa:refID>tcm:4-512883</sqa:refID>
</rdf:Description>

<rdf:Description rdf:about="http://localhost:3333/124">
	<rdf:type rdf:resource="http://data.sqa.org.uk/curriculumNode"/>
	<sqa:cnID>1435</sqa:cnID>
